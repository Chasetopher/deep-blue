<div class="container">

    <div class="d-flex row justify-content-start gx-5">

        {{#each category}}
        <div class="card text-center col-lg-4 col-md-12 col-sm-12 bg-transparent">
            <div class="card-body my-4 btn2">
                <div class="my-5">
                    <span class="textIn1">
                        <a href="/category/{{id}}"><h3>{{category_name}}</h3></a>
                    </span>
                </div>
            </div>
        </div>
        {{/each}}

    </div>

</div>